<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="250dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="@color/LightGrey" android:id="@+id/constraintLayout6"
            app:layout_constraintTop_toTopOf="parent">

        <TextView
                android:text="Paid successfully"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView135" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="32dp" android:textSize="24dp" android:textColor="@color/Black"
                android:textStyle="bold"/>
        <TextView
                android:text="Rs.1200"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_total_paid_txn_detail_slr" android:textSize="24dp"
                android:textColor="@color/Black" android:textStyle="bold"
                android:layout_marginTop="32dp"
                app:layout_constraintTop_toBottomOf="@+id/textView135"
                app:layout_constraintStart_toStartOf="@+id/textView135"/>
        <ImageView
                android:layout_width="100dp"
                android:layout_height="0dp" app:srcCompat="@drawable/check_circle_blue"
                android:id="@+id/imageView5"
                app:layout_constraintTop_toTopOf="@+id/textView135"
                app:layout_constraintBottom_toBottomOf="@+id/tv_total_paid_txn_detail_slr" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="32dp"/>
        <TextView
                android:text="20th Aug 2019, 05:25 PM"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_transaction_date_and_time_txn_detail_slr" android:textSize="18dp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/tv_total_paid_txn_detail_slr"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="32dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
            android:text="Paid for"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView138" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout6" android:textSize="16dp"/>
    <TextView
            android:text="Chicken Biryani"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_dish_name_txn_detail_slr" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView138" android:textSize="20dp" android:textStyle="bold"
            android:textColor="@color/Black"/>
    <TextView
            android:text="(Rs.120)"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_dish_price_txn_detail_slr" android:layout_marginStart="4dp"
            app:layout_constraintStart_toEndOf="@+id/tv_dish_name_txn_detail_slr"
            app:layout_constraintTop_toTopOf="@+id/tv_dish_name_txn_detail_slr" android:textSize="20dp" android:textStyle="bold"
            android:textColor="@color/Black"/>
    <TextView
            android:text="Quantity: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView141" app:layout_constraintEnd_toStartOf="@+id/tv_dish_quantity_txn_detail_slr"
            android:layout_marginEnd="4dp" app:layout_constraintTop_toTopOf="@+id/tv_dish_name_txn_detail_slr" android:textSize="18dp"
            android:textStyle="bold"/>
    <TextView
            android:text="10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_dish_quantity_txn_detail_slr" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView141" android:textSize="18dp" android:textStyle="bold"/>
    <TextView
            android:text="Order ID:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView143" android:textSize="18dp"
            android:textStyle="bold" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_dish_name_txn_detail_slr" android:layout_marginTop="4dp"/>
    <TextView
            android:text="1029384756"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_order_id_txn_detail_slr" android:textSize="18dp"
            android:textStyle="bold" app:layout_constraintStart_toEndOf="@+id/textView143"
            android:layout_marginStart="4dp"
            app:layout_constraintTop_toTopOf="@+id/textView143"/>
    <TextView
            android:text="Paid from"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView147" android:textSize="16dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/textView143"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"/>
    <TextView
            android:text="PAYTM"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_payment_mode_txn_detail_slr" android:textSize="20dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView147" android:layout_marginTop="8dp"
            android:textStyle="bold" android:textColor="@color/SkyBlue"/>
    <TextView
            android:text="Transaction ID:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView149" android:textSize="18dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_payment_mode_txn_detail_slr" android:textStyle="bold"/>
    <TextView
            android:text="1029384756"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_transaction_id_txn_detail_slr" android:textSize="18dp" app:layout_constraintStart_toEndOf="@+id/textView149"
            android:layout_marginStart="4dp"
            app:layout_constraintTop_toTopOf="@+id/textView149" android:textStyle="bold"/>
</androidx.constraintlayout.widget.ConstraintLayout>